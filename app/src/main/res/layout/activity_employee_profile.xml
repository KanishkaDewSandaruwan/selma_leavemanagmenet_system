<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".EmployeeProfile">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Profile">

        <TextView
            android:id="@+id/txtEmpIDProfileStaff"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="32dp"
            android:text="TextView"
            android:textAlignment="viewStart"
            android:textSize="20sp"
            app:layout_constraintEnd_toEndOf="@+id/textView23"
            app:layout_constraintStart_toStartOf="@+id/textView23"
            app:layout_constraintTop_toBottomOf="@+id/textView23" />

        <TextView
            android:id="@+id/txtNameProfileStaff"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="TextView"
            android:textSize="20sp"
            app:layout_constraintEnd_toEndOf="@+id/txtEmpIDProfileStaff"
            app:layout_constraintStart_toStartOf="@+id/txtEmpIDProfileStaff"
            app:layout_constraintTop_toBottomOf="@+id/txtEmpIDProfileStaff" />

        <TextView
            android:id="@+id/txtNicProfileStaff"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="TextView"
            android:textSize="20sp"
            app:layout_constraintEnd_toEndOf="@+id/txtNameProfileStaff"
            app:layout_constraintStart_toStartOf="@+id/txtNameProfileStaff"
            app:layout_constraintTop_toBottomOf="@+id/txtNameProfileStaff" />

        <TextView
            android:id="@+id/txtAddressProfileStaff"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="TextView"
            android:textSize="20sp"
            app:layout_constraintEnd_toEndOf="@+id/txtNicProfileStaff"
            app:layout_constraintStart_toStartOf="@+id/txtNicProfileStaff"
            app:layout_constraintTop_toBottomOf="@+id/txtNicProfileStaff" />

        <TextView
            android:id="@+id/txtEmailProfileStaff"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="TextView"
            android:textSize="20sp"
            app:layout_constraintEnd_toEndOf="@+id/txtAddressProfileStaff"
            app:layout_constraintStart_toStartOf="@+id/txtAddressProfileStaff"
            app:layout_constraintTop_toBottomOf="@+id/txtAddressProfileStaff" />

        <TextView
            android:id="@+id/txtPhoneProfileStaff"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="TextView"
            android:textSize="20sp"
            app:layout_constraintEnd_toEndOf="@+id/txtEmailProfileStaff"
            app:layout_constraintStart_toStartOf="@+id/txtEmailProfileStaff"
            app:layout_constraintTop_toBottomOf="@+id/txtEmailProfileStaff" />

        <TextView
            android:id="@+id/textView23"
            android:layout_width="0dp"
            android:layout_height="76dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="24dp"
            android:layout_marginEnd="16dp"
            android:fontFamily="@font/alfa_slab_one"
            android:text="SELMA "
            android:textAlignment="center"
            android:textColor="#040F4C"
            android:textSize="34sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <Button
        android:id="@+id/btnBackProfileStaff"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="32dp"
        android:backgroundTint="#0E0D0D"
        android:text="Back"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>